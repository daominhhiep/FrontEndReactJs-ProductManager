ount":0,"fixableWarningCount":0,"usedDeprecatedRules":"34"},{"filePath":"62","messages":"63","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"34"},{"filePath":"64","messages":"65","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"34"},"/Users/hiep_dm/WebstormProjects/react-api/src/index.js",[],["66","67"],"/Users/hiep_dm/WebstormProjects/react-api/src/App.js",[],"/Users/hiep_dm/WebstormProjects/react-api/src/reportWebVitals.js",[],"/Users/hiep_dm/WebstormProjects/react-api/src/components/Menu/Menu.js",["68"],"import React, {Component} from 'react';\nimport {Route, Link} from 'react-router-dom'\n\nconst menus = [\n    {\n        name: 'Home',\n        to: '/',\n        exact: true\n    },\n    {\n        name: 'Product Manager',\n        to: '/products',\n        exact: false\n    },\n]\n\nconst MenuLink = ({label, to, activeOnlyWhenExact}) => {\n    return (\n        <Route path={to}\n               exact={activeOnlyWhenExact}\n               children={({match}) => {\n                   let active = match ? 'nav-item' : '';\n                   return (\n                       <li className={active}>\n                           <Link to={to} className=\"nav-link\">\n                               {label}\n                           </Link>\n                       </li>\n                   )\n               }}\n        />\n    )\n}\n\nclass Menu extends Component {\n    render() {\n        return (\n            <nav className=\"navbar navbar-expand-lg navbar-light bg-light\">\n                <div className=\"container-fluid\">\n                    <a className=\"navbar-brand\">API</a>\n                    <button className=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\"\n                            data-bs-target=\"#navbarNav\"\n                            aria-controls=\"navbarNav\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\n                        <span className=\"navbar-toggler-icon\"/>\n                    </button>\n                    <div className=\"collapse navbar-collapse\" id=\"navbarNav\">\n                        <ul className=\"navbar-nav\">\n                            {this.showMenus(menus)}\n                        </ul>\n                    </div>\n                </div>\n            </nav>\n        );\n    }\n\n    showMenus = (menus) => {\n        let result = null;\n        if (menus.length > 0) {\n            result = menus.map((menu, index) => {\n                return (\n                    <MenuLink\n                        key={index}\n                        label={menu.name}\n                        to={menu.to}\n                        activeOnlyWhenExact={menu.exact}\n                    />\n                )\n            });\n        }\n        return result;\n    }\n}\n\nexport default Menu;\n","/Users/hiep_dm/WebstormProjects/react-api/src/components/ProductList/ProductList.js",[],"/Users/hiep_dm/WebstormProjects/react-api/src/components/ProductItem/ProductItem.js",[],"/Users/hiep_dm/WebstormProjects/react-api/src/routes.js",[],"/Users/hiep_dm/WebstormProjects/react-api/src/pages/NotFoundPage/NotFoundPage.js",["69"],"import React, {Component} from 'react';\n\nclass NotFoundPage extends Component {\n    render() {\n        return (\n            <div className=\"container\">\n                <div className=\"alert alert-warning\" role=\"alert\">\n                    A simple warning alert with <a className=\"alert-link\">an example link</a>. Give it a click\n                    if you like.\n                </div>\n            </div>\n        );\n    }\n}\n\nexport default NotFoundPage;\n","/Users/hiep_dm/WebstormProjects/react-api/src/pages/HomePage/HomePage.js",[],"/Users/hiep_dm/WebstormProjects/react-api/src/pages/ProductListPage/ProductListPage.js",["70"],"import React, {Component} from 'react';\nimport ProductList from \"../../components/ProductList/ProductList\";\nimport ProductItem from \"../../components/ProductItem/ProductItem\";\nimport {connect} from \"react-redux\";\nimport callApi from \"../../utils/apiCaller\";\nimport {Link} from \"react-router-dom\";\nimport products from \"../../reducers/products\";\n\nclass ProductListPage extends Component {\n    constructor(props) {\n        super(props);\n        this.state = {\n            products: []\n        }\n    }\n\n    componentDidMount() {\n        callApi('products', 'GET', null).then(res => {\n            this.setState({\n                products: res.data,\n            });\n        });\n    }\n\n    onDelete = (id) => {\n        let { products } = this.state;\n        callApi(`product/${id}`, 'DELETE', null).then(res => {\n            if (res.status === 200) {\n                let index = this.findIndex(products, id);\n                if (index !== -1) {\n                    products.splice(index, 1);\n                    this.setState({\n                        products: products\n                    });\n                }\n            }\n        });\n    }\n\n    findIndex = (products, id) => {\n        let result = -1;\n        products.forEach((product, index) => {\n            if (product.id === id) {\n                result = index\n            }\n        })\n        return result;\n    }\n\n    render() {\n        // let {products} = this.props;\n        let {products} = this.state;\n        return (\n            <div>\n                <div className=\"col-xs-12 col-sm-12 col-lg-12 col-md-12\">\n                    <Link to=\"/product/add\" className=\"btn btn-info mt-4\">Add</Link>\n                    <ProductList>\n                        {this.showProducts(products)}\n                    </ProductList>\n                </div>\n            </div>\n        );\n    }\n\n    showProducts(products) {\n        let result = null;\n        if (products.length > 0) {\n            result = products.map((product, index) => {\n                return (\n                    <ProductItem\n                        key={index}\n                        product={product}\n                        index={index}\n                        onDelete={this.onDelete}\n                    />\n                )\n            });\n        }\n        return result;\n    }\n}\n\nconst mapStateToProps = (state) => {\n    return {\n        products: state.products,\n    }\n}\n\nexport default connect(mapStateToProps, null)(ProductListPage);\n","/Users/hiep_dm/WebstormProjects/react-api/src/pages/ProductActionPage/ProductActionPage.js",["71"],"/Users/hiep_dm/WebstormProjects/react-api/src/reducers/index.js",[],"/Users/hiep_dm/WebstormProjects/react-api/src/reducers/products.js",[],"/Users/hiep_dm/WebstormProjects/react-api/src/utils/apiCaller.js",[],"/Users/hiep_dm/WebstormProjects/react-api/src/constants/Config.js",[],{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":40,"column":21,"nodeType":"78","endLine":40,"endColumn":49},{"ruleId":"76","severity":1,"message":"77","line":8,"column":49,"nodeType":"78","endLine":8,"endColumn":75},{"ruleId":"79","severity":1,"message":"80","line":7,"column":8,"nodeType":"81","messageId":"82","endLine":7,"endColumn":16},{"ruleId":"79","severity":1,"message":"83","line":70,"column":33,"nodeType":"81","messageId":"82","endLine":70,"endColumn":43},"no-native-reassign",["84"],"no-negated-in-lhs",["85"],"jsx-a11y/anchor-is-valid","The href attribute is required for an anchor to be keyboard accessible. Provide a valid, navigable address as the href value. If you cannot provide an href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-valid.md","JSXOpeningElement","no-unused-vars","'products' is defined but never used.","Identifier","unusedVar","'chkbStatus' is assigned a value but never used.","no-global-ass